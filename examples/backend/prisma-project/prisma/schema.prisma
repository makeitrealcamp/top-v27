// This is your Prisma schema file,
// PSL -> Prisma Schema Lenguage
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id          String @id @default(cuid())
  name        String
  description String
  price       Int
}

